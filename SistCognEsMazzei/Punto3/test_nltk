love(paolo,francesca)
exists x.(man(x) & love(x,francesca))
exists z1.(man(z1) & hate(francesca,z1))
(dream(paolo,francesca) & dream(paolo,beatrice) & dream(dante,francesca) & dream(dante,beatrice))
all x.(exists z2.(man(z2) & x(z2) & all y.(man(y) -> (z2 = y))) -> exists z3.(woman(z3) & worship(x,z3)))
exists x.(man(x) & all z5.(exists z4.(woman(z4) & z5(z4) & all y.(woman(y) -> (z4 = y))) -> worship(x,z5)))
sail(caronte)
(exists z6.(hell(z6) & visit(dante,z6)) & exists x.(man(x) & exists z6.(hell(z6) & visit(x,z6))))
(exists x.(woman(x) & exists z7.(gift(z7) & leave(x,z7,virgilio))) & exists z7.(gift(z7) & leave(dante,z7,virgilio)))
exists z9.(star(z9) & see_again(dante,z9) & all y.(star(y) -> (z9 = y)))
